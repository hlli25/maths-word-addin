<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>abc9c60e-03da-4ac2-8266-9f178119b44f</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>hlli25</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Math Equation Editor"/>
  <Description DefaultValue="A powerful equation editor for Microsoft Word with LaTeX support"/>
  <IconUrl DefaultValue="https://hlli25.github.io/maths-word-addin/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://hlli25.github.io/maths-word-addin/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://github.com/hlli25/maths-word-addin"/>
  <AppDomains>
    <AppDomain>https://hlli25.github.io</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://hlli25.github.io/maths-word-addin/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="WordApi" MinVersion="1.3"/>
    </Sets>
  </Requirements>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://hlli25.github.io/maths-word-addin/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://hlli25.github.io/maths-word-addin/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://hlli25.github.io/maths-word-addin/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://github.com/hlli25/maths-word-addin"/>
        <bt:Url id="Commands.Url" DefaultValue="https://hlli25.github.io/maths-word-addin/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://hlli25.github.io/maths-word-addin/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with Math Equation Editor!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Math Equation"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Equation Editor"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Math Equation Editor loaded successfully. Go to the HOME tab and click the 'Equation Editor' button to get started."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Open the Math Equation Editor"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>